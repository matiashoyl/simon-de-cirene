<%= simple_form_for(@curso) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :codigo %>
    <%= f.input :nombre %>
    <%= f.input :fecha_inicio %>
    <%= f.input :fecha_termino %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
